{
  "name": "api",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "ts-node-dev --inspect --ignore-watch node_modules src/server.ts",
    "build": "npx tsc",
    "lint": "eslint src/**/*.{js,ts,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts,json}'",
    "format": "prettier --write 'src/**/*.{js,ts,md,json}' --config ./.prettierrc",
    "prisma-init": "npx prisma generate && npx prisma db push"
  },
  "vercel-build": "npx prisma generate",
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.7",
    "prisma": "^5.15.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.3"
  }
}
